    "model1=LinearRegression()\n",
    "model1.fit(X_train,Y_train)\n",
    "Y_pred = model1.predict(X_test)\n",
    "print('Rsquared of the Linear Regression model is: '+ str(r2_score(Y_pred,Y_test)))"
   ]
